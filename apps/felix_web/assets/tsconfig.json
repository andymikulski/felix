{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "downlevelIteration": true,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "lib": ["DOM", "ES2015", "ScriptHost"],
    "jsx": "react"
  },
  "include": ["js/game/**/*"]
}
