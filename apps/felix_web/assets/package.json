{
  "name": "phaser-starter",
  "version": "0.0.1",
  "description": "A simple Phaser + Typescript starter kit",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base src/ --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "build.esnext": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build.all": "npm run build.prod && npm run build.esnext",
    "test": "karma start karma.config.js",
    "codegen": "ts-node codegen.ts && yarn lint:codegen",
    "lint": "prettier . --write",
    "lint:codegen": "prettier ./**/*.gen.ts --write"
  },
  "keywords": [
    "phaser",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.0",
    "@babel/preset-env": "7.4.1",
    "@babel/preset-typescript": "7.3.3",
    "@types/jasmine": "2.8.7",
    "@types/node": "7.0.0",
    "@types/prop-types": "^15.7.11",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@types/styled-components": "^5.1.34",
    "jasmine-core": "3.1.0",
    "karma": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.0",
    "prettier": "^3.1.1",
    "source-map-loader": "0.2.3",
    "ts-loader": "8.2.0",
    "ts-morph": "20.0.0",
    "tslint": "5.17.0",
    "typescript": "latest",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  },
  "dependencies": {
    "@types/phoenix": "^1.6.4",
    "@types/phoenix_live_view": "^0.18.4",
    "phaser": "3.55.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^6.1.1",
    "tsconfig-paths-webpack-plugin": "3.5.2"
  }
}
